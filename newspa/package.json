    "apollo-link-ws": "^1.0.20",
    "apollo-upload-client": "^13.0.0",
    "apollo-utilities": "^1.3.3",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.1",
    "file-loader": "^6.0.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "i18next": "^19.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
