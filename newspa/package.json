  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/jest/fileMock.js",
      "^(components)$": "<rootDir>/scripts/jest/componentsMock.js",
      "^(mutations)$": "<rootDir>/scripts/jest/actionsMock.js"
    },
    "setupFiles": [
      "<rootDir>/scripts/jest/setupTests.js"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --mode development --host 0.0.0.0 --port 80",
    "test": "jest",
    "lint": "eslint src scripts webpack.config.js",
    "build": "webpack --mode production"
  },
